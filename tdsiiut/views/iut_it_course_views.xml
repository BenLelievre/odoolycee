<odoo>
	<data>
		<record id="iutcourse_action" model="ir.actions.act_window">
			<field name="name">Cours</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">iut.course</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="context">{'search_default_opened':1}</field>
		</record>
		<record id ="view_course_tree" model="ir.ui.view">
			<field name="name">iut_tdsi_course_tree</field>
			<field name="model">iut.course</field>
			<field name="priority">2</field>
			<field name="arch" type="xml">
				<tree string="Course Orders">
					<field name="name" string="Order number"/>
				</tree>
			</field>
		</record>
	</data>
</odoo>